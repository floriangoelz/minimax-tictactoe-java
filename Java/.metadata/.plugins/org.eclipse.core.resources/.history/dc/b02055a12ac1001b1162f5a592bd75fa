package ticTacToe;

public class TicTacToe {

	private int field;
	private int[] players = new int[] {0, 1};
	
	
	private void draw() {
		final String HORIZONTAL = "─────────────";
		String print = "  X  |  O  |  X  ";
		print += HORIZONTAL;
		print += "  X  |  O  |  X  ";
		print += HORIZONTAL;
		print += "  X  |  O  |  X  ";
		System.out.println();
	}
}
